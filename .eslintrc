{
  "extends": ["react-app", "standard"],
  "globals": {
    "setup": true,
    "random": true,
    "dice": true,
    "State": true,
    "randomFloat": true,
    "clone": true
  },
  "rules": {
    "no-var": "warn",
    "prefer-const": "warn",
    "object-shorthand": ["warn", "always"],
    "quote-props": ["warn", "consistent-as-needed"],
    "default-case": "off"
  }
}
